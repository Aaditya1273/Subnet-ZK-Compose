{
  "name": "farmoracle",
  "version": "1.0.0",
  "description": "FarmOracle - Africa's Autonomous AI Farming Oracle on the Blockchain",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python main.py",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm run start",
    "setup": "npm run install-all && npm run setup-backend",
    "install-all": "cd frontend && npm install",
    "setup-backend": "cd backend && pip install -r requirements.txt",
    "deploy": "cd frontend && npm run build && npm run start",
    "backend": "cd backend && python main.py",
    "frontend": "cd frontend && npm run dev"
  },
  "keywords": [
    "blockchain",
    "AI",
    "farming",
    "agriculture",
    "Africa",
    "oracle",
    "smart-contracts",
    "Web3",
    "Next.js"
  ],
  "author": "FarmOracle Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}